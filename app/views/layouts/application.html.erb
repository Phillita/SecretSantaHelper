<!DOCTYPE html>
<html>
  <head>
    <title>
      <%= yield :title %>
    </title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= yield :head %>
  </head>
  <body>
    <%= render 'layouts/nav' %>

    <div class="container-fluid">
      <%= render 'shared/flash_messages' %>
      <%= yield %>
    </div>
  </body>
  <footer class="footer">
    &nbsp;
  </footer>
</html>
